{
    "version": 2,
    "builds": [{
      "src": "app.js",
      "use": "@vercel/node"
    }],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }
  